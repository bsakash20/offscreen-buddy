var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault").default;
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _Button = _interopRequireDefault(require("../../../app/design-system/components/Button"));
var _ThemeProvider = require("../../../app/design-system/providers/ThemeProvider");
var _jsxRuntime = require("react/jsx-runtime");
describe('Button Component', function () {
  var defaultProps = {
    children: 'Button Text',
    onPress: jest.fn()
  };
  beforeEach(function () {
    jest.clearAllMocks();
  });
  describe('Rendering', function () {
    it('renders with default props', function () {
      var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps))
        })),
        getByText = _render.getByText;
      expect(getByText('Button Text')).toBeTruthy();
    });
    it('renders children with correct text', function () {
      var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps))
        })),
        getByText = _render2.getByText;
      expect(getByText('Button Text')).toBeTruthy();
    });
  });
  describe('Button Variants', function () {
    var variants = ['primary', 'secondary', 'outline', 'ghost', 'destructive'];
    variants.forEach(function (variant) {
      it(`renders correctly with ${variant} variant`, function () {
        var _render3 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
            defaultMode: "light",
            children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
              variant: variant
            }))
          })),
          getByText = _render3.getByText;
        expect(getByText('Button Text')).toBeTruthy();
      });
    });
  });
  describe('Button Sizes', function () {
    var sizes = ['sm', 'md', 'lg', 'xl'];
    sizes.forEach(function (size) {
      it(`renders correctly with ${size} size`, function () {
        var _render4 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
            defaultMode: "light",
            children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
              size: size
            }))
          })),
          getByText = _render4.getByText;
        expect(getByText('Button Text')).toBeTruthy();
      });
    });
  });
  describe('Button States', function () {
    it('disables button when disabled prop is true', function () {
      var onPress = jest.fn();
      var _render5 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            onPress: onPress,
            disabled: true
          }))
        })),
        getByText = _render5.getByText;
      expect(getByText('Button Text')).toBeTruthy();
      var buttonElement = _reactNative.screen.getByText('Button Text').parent;
      if (buttonElement) {
        _reactNative.fireEvent.press(buttonElement);
        expect(onPress).not.toHaveBeenCalled();
      }
    });
    it('shows loading state when loading prop is true', function () {
      var onPress = jest.fn();
      var _render6 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            onPress: onPress,
            loading: true
          }))
        })),
        getByText = _render6.getByText;
      expect(getByText('Button Text')).toBeTruthy();
      var buttonElement = _reactNative.screen.getByText('Button Text').parent;
      if (buttonElement) {
        _reactNative.fireEvent.press(buttonElement);
        expect(onPress).not.toHaveBeenCalled();
      }
    });
  });
  describe('Interaction', function () {
    it('calls onPress when button is pressed', function () {
      var _render7 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps))
        })),
        getByText = _render7.getByText;
      var buttonElement = getByText('Button Text');
      _reactNative.fireEvent.press(buttonElement);
      expect(defaultProps.onPress).toHaveBeenCalledTimes(1);
    });
    it('does not call onPress when disabled', function () {
      var _render8 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            disabled: true
          }))
        })),
        getByText = _render8.getByText;
      var buttonElement = getByText('Button Text');
      _reactNative.fireEvent.press(buttonElement);
      expect(defaultProps.onPress).not.toHaveBeenCalled();
    });
    it('does not call onPress when loading', function () {
      var _render9 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            loading: true
          }))
        })),
        getByText = _render9.getByText;
      var buttonElement = getByText('Button Text');
      _reactNative.fireEvent.press(buttonElement);
      expect(defaultProps.onPress).not.toHaveBeenCalled();
    });
  });
  describe('Custom Styling', function () {
    it('applies custom style when style prop is provided', function () {
      var customStyle = {
        backgroundColor: 'red',
        padding: 20
      };
      var _render0 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            style: customStyle
          }))
        })),
        getByText = _render0.getByText;
      expect(getByText('Button Text')).toBeTruthy();
    });
    it('applies custom text style when textStyle prop is provided', function () {
      var customTextStyle = {
        fontSize: 20,
        color: 'blue'
      };
      var _render1 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            textStyle: customTextStyle
          }))
        })),
        getByText = _render1.getByText;
      var textElement = getByText('Button Text');
      expect(textElement.props.style).toContainEqual(customTextStyle);
    });
  });
  describe('Accessibility', function () {
    it('has correct accessibility role', function () {
      var _render10 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps))
        })),
        getByText = _render10.getByText;
      var buttonElement = getByText('Button Text');
      var parent = buttonElement.parent;
      if (parent) {
        expect(parent.props.accessibilityRole).toBe('button');
      }
    });
    it('sets accessibility label when provided', function () {
      var accessibilityLabel = 'Custom button label';
      var _render11 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            accessibilityLabel: accessibilityLabel
          }))
        })),
        getByText = _render11.getByText;
      var buttonElement = getByText('Button Text');
      var parent = buttonElement.parent;
      if (parent) {
        expect(parent.props.accessibilityLabel).toBe(accessibilityLabel);
      }
    });
    it('supports testID for testing', function () {
      var testID = 'custom-button-test-id';
      var _render12 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            testID: testID
          }))
        })),
        getByTestId = _render12.getByTestId;
      expect(getByTestId(testID)).toBeTruthy();
    });
  });
  describe('Theme Support', function () {
    it('applies theme-based styling correctly in light mode', function () {
      var _render13 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            variant: "primary"
          }))
        })),
        getByText = _render13.getByText;
      expect(getByText('Button Text')).toBeTruthy();
    });
    it('adapts to dark theme when specified', function () {
      var _render14 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "dark",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            variant: "primary"
          }))
        })),
        getByText = _render14.getByText;
      expect(getByText('Button Text')).toBeTruthy();
    });
  });
  describe('Full Width', function () {
    it('applies full width style when fullWidth prop is true', function () {
      var _render15 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            fullWidth: true
          }))
        })),
        getByText = _render15.getByText;
      expect(getByText('Button Text')).toBeTruthy();
    });
  });
  describe('Error Handling', function () {
    it('handles onPress errors gracefully', function () {
      var onPressError = jest.fn(function () {
        throw new Error('Button press failed');
      });
      var _render16 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            onPress: onPressError
          }))
        })),
        getByText = _render16.getByText;
      var buttonElement = getByText('Button Text');
      expect(function () {
        _reactNative.fireEvent.press(buttonElement);
      }).toThrow();
    });
  });
  describe('Mobile Optimization', function () {
    it('supports haptic feedback for interactions', function () {
      var mockImpactAsync = jest.fn();
      jest.doMock('expo-haptics', function () {
        return {
          impactAsync: mockImpactAsync,
          ImpactFeedbackStyle: {
            Light: 'light'
          }
        };
      });
      var _render17 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            haptic: true
          }))
        })),
        getByText = _render17.getByText;
      var buttonElement = getByText('Button Text');
      _reactNative.fireEvent.press(buttonElement);
      expect(defaultProps.onPress).toHaveBeenCalled();
      expect(mockImpactAsync).toHaveBeenCalledWith('light');
    });
  });
  describe('Event Handlers', function () {
    it('calls onPressIn when button is pressed in', function () {
      var onPressIn = jest.fn();
      var _render18 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            onPressIn: onPressIn
          }))
        })),
        getByText = _render18.getByText;
      var buttonElement = getByText('Button Text');
      (0, _reactNative.fireEvent)(buttonElement, 'onPressIn');
      expect(onPressIn).toHaveBeenCalledTimes(1);
    });
    it('calls onPressOut when button is released', function () {
      var onPressOut = jest.fn();
      var _render19 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ThemeProvider.ThemeProvider, {
          defaultMode: "light",
          children: (0, _jsxRuntime.jsx)(_Button.default, Object.assign({}, defaultProps, {
            onPressOut: onPressOut
          }))
        })),
        getByText = _render19.getByText;
      var buttonElement = getByText('Button Text');
      (0, _reactNative.fireEvent)(buttonElement, 'onPressOut');
      expect(onPressOut).toHaveBeenCalledTimes(1);
    });
  });
});
