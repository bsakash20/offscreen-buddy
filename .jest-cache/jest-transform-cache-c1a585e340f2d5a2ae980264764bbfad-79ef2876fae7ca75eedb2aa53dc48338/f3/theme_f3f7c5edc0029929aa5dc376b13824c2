var _colors = require("./colors");
var _typography = require("./typography");
var _spacing = require("./spacing");
var _breakpoints = require("./breakpoints");
var createTheme = exports.createTheme = function createTheme(mode) {
  var isDark = mode === 'dark';
  return {
    colors: isDark ? _colors.colorTokens.dark : _colors.colorTokens.light,
    typography: _typography.typographyTokens,
    spacing: _spacing.spacingTokens,
    breakpoints: _breakpoints.breakpointTokens,
    animation: {
      duration: {
        fast: 150,
        normal: 250,
        slow: 350
      },
      easing: {
        easeOut: 'cubic-bezier(0.16, 1, 0.3, 1)',
        easeIn: 'cubic-bezier(0.4, 0, 1, 1)',
        easeInOut: 'cubic-bezier(0.4, 0, 0.2, 1)'
      }
    },
    elevation: {
      small: {
        shadowColor: isDark ? '#000000' : '#000000',
        shadowOffset: {
          width: 0,
          height: 1
        },
        shadowOpacity: isDark ? 0.25 : 0.05,
        shadowRadius: 2,
        elevation: 2
      },
      medium: {
        shadowColor: isDark ? '#000000' : '#000000',
        shadowOffset: {
          width: 0,
          height: 2
        },
        shadowOpacity: isDark ? 0.35 : 0.1,
        shadowRadius: 4,
        elevation: 4
      },
      large: {
        shadowColor: isDark ? '#000000' : '#000000',
        shadowOffset: {
          width: 0,
          height: 4
        },
        shadowOpacity: isDark ? 0.5 : 0.15,
        shadowRadius: 8,
        elevation: 8
      }
    },
    borderRadius: {
      small: 4,
      medium: 8,
      large: 12,
      extraLarge: 16,
      full: 9999
    },
    opacity: {
      disabled: 0.4,
      muted: 0.6,
      faint: 0.8
    },
    zIndex: {
      hide: -1,
      auto: 'auto',
      base: 0,
      docked: 10,
      dropdown: 1000,
      sticky: 1100,
      banner: 1200,
      overlay: 1300,
      modal: 1400,
      popover: 1500,
      skipLink: 1600,
      toast: 1700,
      tooltip: 1800
    }
  };
};
var lightTheme = exports.lightTheme = createTheme('light');
var darkTheme = exports.darkTheme = createTheme('dark');
var themeUtils = exports.themeUtils = {
  getSystemTheme: function getSystemTheme() {
    if (typeof window !== 'undefined' && window.matchMedia) {
      return window.matchMedia('(prefers-color-scheme: dark)').matches;
    }
    return false;
  },
  getEffectiveTheme: function getEffectiveTheme(mode, systemIsDark) {
    if (mode === 'system') {
      return systemIsDark ? 'dark' : 'light';
    }
    return mode;
  },
  createResponsiveTheme: function createResponsiveTheme(baseTheme, width, height) {
    var isTablet = width >= _breakpoints.breakpointTokens.scale.md;
    var isLandscape = width > height;
    return Object.assign({}, baseTheme, {
      spacing: Object.assign({}, baseTheme.spacing, {
        scale: Object.assign({}, baseTheme.spacing.scale, {
          xs: isTablet ? 6 : baseTheme.spacing.scale.xs,
          sm: isTablet ? 10 : baseTheme.spacing.scale.sm,
          md: isTablet ? 20 : baseTheme.spacing.scale.md,
          lg: isTablet ? 30 : baseTheme.spacing.scale.lg,
          xl: isTablet ? 40 : baseTheme.spacing.scale.xl
        })
      }),
      typography: Object.assign({}, baseTheme.typography, {
        text: Object.assign({}, baseTheme.typography.text, {
          body: {
            base: isTablet ? Object.assign({}, baseTheme.typography.text.body.base, {
              fontSize: baseTheme.typography.text.body.base.fontSize + 1
            }) : baseTheme.typography.text.body.base,
            sm: isTablet ? Object.assign({}, baseTheme.typography.text.body.sm, {
              fontSize: baseTheme.typography.text.body.sm.fontSize + 1
            }) : baseTheme.typography.text.body.sm,
            lg: isTablet ? Object.assign({}, baseTheme.typography.text.body.lg, {
              fontSize: baseTheme.typography.text.body.lg.fontSize + 1
            }) : baseTheme.typography.text.body.lg
          }
        })
      })
    });
  },
  supportsFeature: function supportsFeature(theme, feature) {
    switch (feature) {
      case 'glassmorphism':
        return true;
      case 'gradients':
        return true;
      case 'shadows':
        return true;
      default:
        return false;
    }
  }
};
var validateTheme = exports.validateTheme = function validateTheme(theme) {
  try {
    if (!theme.colors || !theme.typography || !theme.spacing) {
      console.error('Theme validation failed: missing core tokens');
      return false;
    }
    if (!theme.colors.system || !theme.colors.brand) {
      console.error('Theme validation failed: missing color tokens');
      return false;
    }
    if (theme.spacing.scale.xs <= 0 || theme.spacing.scale.sm <= 0) {
      console.error('Theme validation failed: invalid spacing values');
      return false;
    }
    return true;
  } catch (error) {
    console.error('Theme validation error:', error);
    return false;
  }
};
