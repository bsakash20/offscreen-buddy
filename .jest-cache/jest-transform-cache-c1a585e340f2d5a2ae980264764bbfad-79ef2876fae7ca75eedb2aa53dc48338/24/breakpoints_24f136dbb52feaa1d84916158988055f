var responsiveDesign = exports.responsiveDesign = {
  createResponsiveValue: function createResponsiveValue(values) {
    var filledValues = Object.assign({}, values);
    var breakpoints = ['xs', 'sm', 'md', 'lg', 'xl', '2xl'];
    var defaultValue = values[breakpoints[0]];
    for (var i = 1; i < breakpoints.length; i++) {
      var current = breakpoints[i];
      var previous = breakpoints[i - 1];
      if (filledValues[current] === undefined) {
        filledValues[current] = filledValues[previous] || defaultValue;
      }
    }
    for (var breakpoint of breakpoints) {
      if (filledValues[breakpoint] === undefined) {
        filledValues[breakpoint] = defaultValue;
      }
    }
    return filledValues;
  },
  getResponsiveFontSize: function getResponsiveFontSize(baseSize) {
    var isTablet = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : false;
    if (isTablet) {
      return baseSize * 1.1;
    }
    return baseSize;
  },
  getResponsiveSpacing: function getResponsiveSpacing(baseSpacing) {
    var isTablet = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : false;
    var isLandscape = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : false;
    if (isTablet) {
      return baseSpacing * 1.25;
    }
    if (isLandscape) {
      return baseSpacing * 1.1;
    }
    return baseSpacing;
  }
};
var responsivePatterns = exports.responsivePatterns = {
  getColumnCount: function getColumnCount(width) {
    if (width < mobileBreakpoints.sm) return 1;
    if (width < mobileBreakpoints.md) return 2;
    if (width < mobileBreakpoints.lg) return 3;
    if (width < mobileBreakpoints.xl) return 4;
    return 6;
  },
